import turtle
from turtle import *

my_turtle = turtle.Turtle()

def square():
    my_turtle.forward(50)
    my_turtle.left(90)
    my_turtle.forward(50)
    my_turtle.left(90)
    my_turtle.forward(50)
    my_turtle.left(90)
    my_turtle.forward(50)


for i in range(50):
    square()
    my_turtle.forward(i+5)

done()
